{
  "name": "cueflow-migration",
  "version": "1.0.0",
  "description": "Migration script from Supabase to Firebase Firestore for CueFlow app",
  "main": "migrate-to-firestore.js",
  "scripts": {
    "migrate": "node migrate-to-firestore.js",
    "migrate-data": "node migrate-data-to-firestore.js",
    "migrate-cueflow-data": "node migrate-cueflow-data.js",
    "setup": "node setup-firestore.js",
